{
  "name": "intro-storybook-react-template",
  "version": "0.2.0",
  "description": "Starter template to get up and running quickly with React and Storybook",
  "author": "Chromatic <https://chromatic.com/>",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/chromaui/intro-storybook-react-template"
  },
  "bugs": {
    "url": "https://github.com/chromaui/intro-storybook-react-template/issues"
  },
  "engines": {
    "pnpm": ">=8.6"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "scripts": {
    "build": "vite build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "init-msw": "msw init public/",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "lint": "eslint --ext ts,tsx .",
    "format": "prettier --write './**/*.{js,ts,tsx,css,scss,json,html,graphql,md}' --ignore-path .gitignore",
    "gen-c": "scaffdog generate react-component",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "7.5.2",
    "@storybook/addon-interactions": "7.5.2",
    "@storybook/addon-links": "7.5.2",
    "@storybook/blocks": "7.5.2",
    "@storybook/jest": "0.2.3",
    "@storybook/react": "7.5.2",
    "@storybook/react-vite": "7.5.2",
    "@storybook/testing-library": "0.2.2",
    "@types/react": "18.2.34",
    "@types/react-dom": "18.2.14",
    "@typescript-eslint/eslint-plugin": "6.9.1",
    "@typescript-eslint/parser": "6.9.1",
    "@vitejs/plugin-react": "4.1.1",
    "eslint": "8.52.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-storybook": "0.6.15",
    "eslint-plugin-unused-imports": "3.0.0",
    "husky": "^8.0.0",
    "lint-staged": "14.0.1",
    "msw": "1.3.2",
    "msw-storybook-addon": "1.10.0",
    "prettier": "3.0.3",
    "prop-types": "15.8.1",
    "scaffdog": "3.0.0",
    "storybook": "7.5.2",
    "typescript": "5.2.2",
    "vite": "4.5.0"
  }
}
